<?xml version="1.0"?>
<map proto="1.3.3">
<name>Cyanide</name>
<version>1.0</version>
<objective>Kill as many ennemies as possible.</objective>
<authors>
    <author>Shredded_PvP</author>
    <author>Mario3DS957</author>
</authors>
<kits>
    <kit name="spawn">
        <item slot="0" damage="-3000">stone sword</item>
        <item slot="1" enchantment="arrow infinite: 1" damage="-3000">bow</item>
        <item slot="6" damage="-3000">stick</item>
        <item slot="7" damage="-3000">golden apple</item>
        <item slot="8" damage="-3000" amount="64">golden carrot</item>
    </kit>
    <kit name="red" parents="spawn">
        <helmet color="cd0000" damage="-3000">leather helmet</helmet>
        <chestplate color="cd0000" damage="-3000">leather chestplate</chestplate>
        <leggings color="cd0000" damage="-3000">leather leggings</leggings>
        <boots color="cd0000" damage="-3000">leather boots</boots>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet color="0066cc" damage="-3000">leather helmet</helmet>
        <chestplate color="0066cc" damage="-3000">leather chestplate</chestplate>
        <leggings color="0066cc" damage="-3000">leather leggings</leggings>
        <boots color="0066cc" damage="-3000">leather boots</boots>
    </kit>
</kits>
<teams>
    <team color="dark red" max="25" max-overfill="30">Red Team</team>
    <team color="blue" max="25" max-overfill="30">Red Team</team>
</teams>
<spawns>
    <spawn team="red" yaw="-90" kit="red">
        <point>14,79,0</point>
    </spawn>
    <spawn team="blue" yaw="90" kit="blue">
        <point>-14,79,0</point>
    </spawn>
    <default yaw="-90">
        <point>96,74,0</point>
    </default>
</spawns>
<portals>
    <portal x="@34" y="@68" z="@0" yaw="@90">
        <cuboid min="36,83,-2" max="36,79,-3"/>
    </portal>
    <portal x="@26" y="@37" z="@-33" yaw="@-90">
        <cuboid min="36,83,2" max="26,79,23"/>
    </portal>
    <portal x="@34" y="@68" z="@0" yaw="@90">
        <cuboid min="34,40,-23" max="34,36,-23"/>
    </portal>
    <portal x="@-33" y="@68" z="@0" yaw="@-90">
        <cuboid min="-35,79,3" max="-35,83,2"/>
    </portal>
    <portal x="@-27" y="@37" z="@34" yaw="@90">
        <cuboid min="-35,79,-1" max="-35,83,-2"/>
    </portal>
    <portal x="@-33" y="@68" z="@0" yaw="@-90">
        <cuboid min="-33,36,24" max="-33,40,25"/>
    </portal>
</portals>
<regions>
    <apply block="deny-all">
        <rectangle name="main" min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<killreward>
    <item>golden apple</item>
    <item>iron ingot</item>
</killreward>
<score>
    <time>600</time>
</score>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather chestplate</item>
    <item>leather helmet</item>
    <item>leather boots</item>
    <item>leather leggings</item>
    <item>stick</item>
    <item>golden apple</item>
    <item>golden carrot</item>
</itemremove>
</map>