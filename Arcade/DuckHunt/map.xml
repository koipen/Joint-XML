<?xml version="1.0"?>
<map proto="1.3.3">
    <name>Duck Hunt</name>
    <version>0.1</version>
    <objective>The shooters must kill all of the ducks before the time runs out!</objective>
    <authors>
        <author>Captain_Elliott</author>
    </authors>
    <teams>
        <team color="white" max="20" max-overfill="25">Ducks</team>
        <team color="gray" max="16" max-overfill="16">Shooters</team>
    </teams>
    <rage/>
    <blitz>
        <time>3m</time>
        <lives>1</lives>
        <title>Duck Hunt</title>
    </blitz>
    <time result="Ducks">3m</time>
    <kits force="ture" potion-particles="false">
        <kit name="shooters">
            <item slot="0" enchantment="arrow damage:10; arrow infinite:1" name="`8Pistol `41 Shot, 1 Kill">bow</item>
            <item slot="28">arrow</item>
            <potion duration="oo" amplifier="10">damage resistance</potion>
        </kit>
        <kit name="ducks">
            <potion duration="oo" amplifier="2">speed</potion>
        </kit>
    </kits>
    <spawns>
        <spawn team="ducks" yaw="90">
            <point>39,6,0</point>
        </spawn>
        <spawn team="shooters" yaw="180">
            <point>-38,5,48</point>
            <point>-33,5,48</point>
            <point>-28,5,48</point>
            <point>-23,5,48</point>
            <point>-18,5,48</point>
            <point>-12,5,48</point>
            <point>-7,5,48</point>
            <point>-2,5,48</point>
            <point>2,5,48</point>
            <point>7,5,48</point>
            <point>12,5,48</point>
            <point>17,5,48</point>
            <point>22,5,48</point>
            <point>27,5,48</point>
            <point>32,5,48</point>
            <point>37,5,48</point>
        </spawn>
        <default yaw="0">
            <point>0,44,0</point>
        </default>
    </spawns>
    <filters>
        <filter name="deny-ducks">
            <not>
                <team>ducks</team>
            </not>
        </filter>
    </filters>
    <regions>
        <apply block="deny-all">
            <rectangle min="oo,oo" max="-oo,-oo"/>
        </apply>
        <apply enter="deny-ducks">
            <negative>
                <cuboid min="-39,6,0" max="40,31,-1"/>
            </negative>
        </apply>
    </regions>
    <mob-disguises>
        <disguise type="chicken">
            <filter>
                <team>ducks</team>
            </filter>
        </disguise>
    </mob-disguises>
</map>
